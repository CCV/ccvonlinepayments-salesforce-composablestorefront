<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="CCVPayment">
        <url>https://vpos-test.jforce.be/vpos/api/v1</url>
        <!-- TODO: should we put the api-key as password? -->
        <user-id>API_KEY_CCV</user-id>
        <password encrypted="true" encryption-type="common.export"></password>
    </service-credential>

    <service-profile service-profile-id="CCVPayment">
        <timeout-millis>30000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>true</cb-enabled>
        <cb-calls>50</cb-calls>
        <cb-millis>100000</cb-millis>
    </service-profile>

    <service service-id="CCVPayment">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix/>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>CCVPayment</profile-id>
        <credential-id>CCVPayment</credential-id>
    </service>

</services>

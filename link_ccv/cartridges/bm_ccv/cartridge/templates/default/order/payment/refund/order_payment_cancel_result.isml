<isdecorate template="pt_csc.isml">
    <isif condition="${pdict.success}">
        <div class="alert alert-success">
            Cancel request successfully submitted.
        </div>
    <iselse>
        <div class="alert alert-danger">
            Refund failed:

            <div>
             <isprint value="${pdict.errorMessage}">
            </div>

        </div>
    </isif>

</isdecorate>
